/****************** 编辑框 *****************/
.edittable {margin:0 auto;*margin-top:-20px;}
.edittable sup {color:red;font-family:宋体;font-size:12px;}
.edittable label{font-weight:normal;}
.edittable tr{height:40px;}
.edittable td{white-space:nowrap;font-size:14px;dfont-family:宋体}
.edittable span{display:inline-block;width:24px;text-align:center;}
.edittable input[type=text],.edittable input[type=number],.edittable select {width:80px;height:24px;line-height:24px;border:1px solid #ddd;font-size:12px;font-family:宋体;padding-left:3px;vertical-align:middle;}
.edittable input:hover,.edittable select:hover,.edittable input:focus,.edittable select:focus
{	border:#35a5e5 1px solid;
	background:#f7f7f7;
	box-shadow:0 0 5px rgba(81, 203, 238, 1);
	-webkit-box-shadow: 0 0 5px rgba(81, 203, 238, 1);
	-moz-box-shadow: 0 0 5px rgba(81, 203, 238, 1);
}

.editul {margin:0px;}
.editul li {height:40px;padding-left:40px;border-bottom:1px dotted #ddd;}
.editul li:last-child{border-bottom:0px;}
.editul li span {display:inline-block;width:65px;line-height:40px;color:#777}
.editul input,.editul select {height:26px;width:200px;border:1px solid #ddd;line-height:26px;padding-left:5px;}
.editul input:hover,.editul select:hover,.editul input:focus,.editul select:focus
{	border:#35a5e5 1px solid;
	background:#f7f7f7;
	box-shadow:0 0 5px rgba(81, 203, 238, 1);
	-webkit-box-shadow: 0 0 5px rgba(81, 203, 238, 1);
	-moz-box-shadow: 0 0 5px rgba(81, 203, 238, 1);
}


