<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>交易通知</title>
    <link href="/admin/css/bootstrap.min.css" rel="stylesheet">
    <!-- jqgrid-->
    <link href="/admin/css/ui.jqgrid.css" rel="stylesheet">
    <link href="/admin/css/animate.min.css" rel="stylesheet">
    <link href="/admin/css/style.min.css" rel="stylesheet">
    <style>
    /*搜索框样式重写*/
    #searchmodfbox_table_list_2{
        position:absolute !important;
        top:302px !important;
        left:603px !important;
    }
    </style>
</head>
<body class="gray-bg" style="overflow:hidden">
<div class="animated fadeInRight">
    <div class="row">
        <div class="col-sm-12">
            <div class="ibox ">
                <div class="ibox-content">
                    <h4 class="m-t">交易通知</h4>
                    <div class="jqGrid_wrapper">
                        <table id="table_list_2"></table>
                        <div id="pager_list_2"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- 全局js -->
<script src="/admin/js/jquery-2.1.1.min.js"></script>
<script src="/admin/js/bootstrap.min.js?v=3.4.0"></script>

<!-- jqGrid -->
<script src="/admin/js/jqGrid/grid.locale-cn.js"></script>
<script src="/admin/js/jqGrid/jquery.jqGrid.min.js"></script>

<!-- 自定义js -->
<script src="/admin/js/content.min.js"></script>
<script>
    $(document).ready(function()
    {
        $.jgrid.defaults.styleUI="Bootstrap";
        $("#table_list_2").jqGrid({
            url:"/admin/home/getResponselog",
            datatype:"json",
            height:700,
            autowidth:true,
            shrinkToFit:true,
            rowNum:19,
            colNames:["序号","通知类型","版本号","交易金额","交易状态","交易创建时间","交易支付时间","交易关闭时间","银行名称","处理结果"],
            colModel:
                    [   //默认不可编辑 可搜索 可排序
                        {name:"ID",index:"ID",sorttype:"int",key:true,width:20},
                        {name:"notify_type",index:"notify_type",sortable:false,width:50},
                        {name:"version",index:"version",sortable:false,width:50},
                        {name:"trade_amount",index:"trade_amount",sortable:false,width:50},
                        {name:"trade_status",index:"trade_status",sortable:false,width:50},
                        {name:"gmt_create",index:"gmt_create",sorttype:"date",width:100},
                        {name:"gmt_payment",index:"gmt_payment",sorttype:"date",width:100},
                        {name:"gmt_close",index:"gmt_close",sorttype:"date",width:100},
                        {name:"bankName",index:"bankName",sortable:false,width:50},
                        {name:"result",index:"result",sortable:false,width:50}
                    ],
            pager:"#pager_list_2",
            loadonce:true,
            viewrecords:true
        });
        $("#table_list_2").jqGrid("navGrid","#pager_list_2",{edit:false,add:false,del:false,search:true},{closeAfterEdit:true,reloadAfterSubmit:false});

        $(window).bind("resize",function(){var b=$(".jqGrid_wrapper").width();$("#table_list_2").setGridWidth(b)})
    });
</script>
</body>
</html>