<!doctype html>
<html ng-app="myapp">
<head>
    <meta charset="utf-8" />
    <title>货盘列表</title>
    <!---
        <script src="/admin/js/jquery-2.1.1.min.js"></script>
        <link rel="stylesheet" href="http://apps.bdimg.com/libs/jqueryui/1.10.4/css/jquery-ui.min.css">
        <script src="http://apps.bdimg.com/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
        --->
    <link rel="stylesheet" href="http://apps.bdimg.com/libs/jqueryui/1.10.4/css/jquery-ui.min.css">
    <script src="http://apps.bdimg.com/libs/jquery/1.10.2/jquery.min.js"></script>
    <script src="http://apps.bdimg.com/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>

    <!-- angularjs -->
    <script src="/admin/js/angular.min.js"></script>
    <script src="/admin/js/angular-animate.min.js"></script>
    <!--分页插件-->
    <script src="/admin/kindeditor-4.1.7/angular-ui-bootstrap.js"></script>

    <link href="/admin/css/animate.min.css" rel="stylesheet">

    <!--- bootstrap --->
    <script src="/admin/js/bootstrap.min.js"></script>
    <link href="/admin/css/bootstrap.min.css" rel="stylesheet">

    <!--- 后台公共样式表 --->
    <link href="/admin/css/style.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/common.css" />

    <!--- 原生JS与angularJS工具箱,工具箱通用对话框CSS --->
    <script src="/js/utils.js"></script>
    <link href="/css/zh-msg.css" rel="stylesheet">

    <script src="/js/syzxModule.js"></script>

    <style>
        .newseditform
        {	position:absolute;
            top:50%;
            left:50%;
            width:800px;
            height:640px;
            margin-left:-300px;
            margin-top:-400px;
            border:1px solid #ddd;
            background:#eee;
            visibility:hidden;
        }
        .newseditform #headbar { padding-left:10px;height:40px;line-height:40px;background:#1E90FF;color:white;font-size:18px;}
        .newseditform #editform { padding-left:20px;padding-right:20px; }
        .newseditform input[type=text] { width:730px; margin-bottom:10px;}
        input[type=checkbox]{width: 16px;height: 16px;}
        .newseditform textarea{width:730px;height:460px;visibility:hidden;}

        /*编辑框样式*/
        .contentStyle,.contentStyle p ,.contentStyle label{font-size: 16px;font-family: "宋体","Microsoft YaHei",Arial,sans-serif;}
        .contentStyle > div{margin-bottom: 10px;height:35px;}
        .contentStyle input{height: 35px;border-radius: 3px;}
        .modal-body{padding-left:0px;}
        .modal-body select{height: 33px;}

        input{border: 1px solid #ddd;}

        #inputSelect{width: 100%;display: block;}
        #inputSelect ul{position: absolute;top: 35px;right:0;z-index: 111;width: 269px;border: 1px solid #ccc;display: none;}
        #inputSelect li{background-color: #fff;padding: 4px;text-indent: 5px;}
        #inputSelect .inputLiAct{background-color: #ddd;}
        /*添加/编辑中多行文字对齐*/
        .contentStyle label{height:24px;line-height:24px;width:115px;text-align:justify;display:inline-block;overflow:hidden;vertical-align:top;margin: 6px 8px;}
        .contentStyle label i{display:inline-block;width:100%;height:0;}
        .contentStyle label span{color: red;position: absolute;top: 0;right: 6px;}
    </style>
</head>

<body ng-controller="mainCtrl as cBox" class="white-bg" style="overflow:hidden;padding:20px;">

<div class="animated fadeInRight" id="fade">

    <div style="float:left;margin-bottom:10px">
        <button ng-click="add()" class="btn btn-default"><i class="glyphicon glyphicon-plus"></i> 新增</button>
        <button ng-click="search()" class="btn btn-warning"><i class="glyphicon glyphicon-search"></i> 查找</button>
        <button ng-click="del()" class="btn btn-danger"><i class="glyphicon glyphicon-trash"></i> 删除</button>
        <button ng-click="refresh()" class="btn btn-info"><i class="glyphicon glyphicon-refresh"></i> 刷新</button>
    </div>

    <ul style="margin:0px;float:right;margin-bottom:10px;font-family:宋体;"
        class="pagination-sm"

        uib-pagination
        boundary-links="true"

        previous-text="<"
        next-text=">"
        first-text="|<"
        last-text=">|"

        ng-change="pageChange();"
        ng-model="currentPage"

        force-ellipses="true"
        rotate="true"
        items-per-page="{# $ROWS_PER_PAGE #}"
        max-size="5"
        total-items="SupplyNumber">
    </ul>

    <table class="datalist">
        <thead><tr>
            <td width="50">序号</td>
            <td width="30" ng-click="selectAll()" title="选择全部/取消选择"><img src="/admin/images/checkbox.gif"></td>
            <td width="50">货主</td>
            <td width="60">货物</td>
            <td width="80">包装形式</td>
            <td width="70">发货数量</td>
            <td width="95">数量偏差</td>
            <td width="90">海/河运</td>
            <td width="100">单价(RMB)</td>
            <td width="95">出发港</td>
            <td width="100">最早起运期</td>
            <td width="100">最迟起运期</td>
            <td width="80">到达港</td>
            <td width="70">付款方式</td>
            <td width="70">是否代理</td>
            <td width="70">货盘状态</td>
        </tr></thead>
        <tbody><tr ng-repeat="item in list track by $index" ng-click="show(item.ID)">
            <td align="center">{{item.ID}}</td>
            <td align="center"><input type="checkbox" ng-checked="item.Checked == 1" ng-model="item.Checked"></td>
            <td>{{item.UserName}}</td>
            <td align="right">{{item.GoodsName}}</td>
            <td align="right">{{item.PackageMethod}}</td>
            <td align="right">{{item.Qty | number:0}}</td>
            <td align="right">{{item.QtyDeviation}}</td>
            <td align="right">{{item.SeaOrRiver}}</td>
            <td align="right">{{item.Price | currency:"￥"}}</td>
            <td align="right">{{item.FromPortName}}</td>
            <td align="right">{{item.LoadDateFrom | dateformat}}</td>
            <td align="right">{{item.LoadDateTo | dateformat}}</td>
            <td align="right">{{item.ToPortName}}</td>
            <td align="right">{{item.PaymentMethod}}</td>
            <td align="right">{{item.NeedAgent}}</td>
            <td align="center">{{item.State}}</td>
        </tr></tbody>
    </table>

</div>

<!--添加或编辑-->
<div class="modal dhide fade in" id="myModal" style="margin-top:20px;"  aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog" style="width:450px;"><div class="modal-content">
        <div class="modal-header" style="height:60px;padding-top:0px;">
            <button class="close" type="button" data-dismiss="modal" style="margin-top:5px;font-size:48px;">×</button>
            <h3>{{oneSupply.ID == 0 ? "添加" : "编辑"}}船期</h3>
        </div>
        <div class="modal-body" style="height:640px;">
            <form name="content" class="contentStyle" >
                <div class="col-md-12" id="userSelectBox">
                    <label class='col-sm-4 muted'>货主<i></i><span>*</span></label>
                    <div class="input-group col-sm-8">
                        <input type="text" placeholder="请输入用户名或手机号" class="form-control userName" id="userSelectText" ng-change="showUserDlg(oneSupply.UserName)" ng-model="oneSupply.UserName" >
                        <div id="inputSelect">
                            <ul id="inputSelectList">
                                <li ng-repeat="item in inputSelect track by $index" ng-click="inputSelectList($index)" tabindex="1">{{item.Name}}</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-12"><label class='col-sm-4'>货物<i></i><span>*</span></label><input class='col-sm-8 goodsName' ng-model="oneSupply.GoodsName" ></div>
                <div class="col-md-12"><label class='col-sm-4'>包装形式<i></i><span>*</span></label>
                    <select id="select_pack" class="col-sm-8" ng-model="selected_pack" ng-options="s.id as s.pack for s in packageMethod">
                    </select></div>
                <div class="col-md-12"><label class='col-sm-4'>发货数量<i></i><span>*</span></label><input class='col-sm-8 qty' ng-model="oneSupply.Qty"></div>

                <div class="col-md-12" id="fromPortBox">
                    <label class='col-sm-4'>出发港<i></i><span>*</span></label>
                    <input class='col-sm-8 portEdit' id="fromPort" type="text" ng-click="showPortDlg(0);" ng-model="oneSupply.FromPortName">
                </div>
                <div class="col-md-12" id="toPortBox">
                    <label class='col-sm-4'>到达港<i></i><span>*</span></label>
                    <input class='col-sm-8 portEdit' id="toPort" type="text" ng-click="showPortDlg(1);" ng-model="oneSupply.ToPortName">
                </div>
                <div class="col-md-12"><label class='col-sm-4'>数量偏差<i></i></label><input class='col-sm-8 qtyDeviation' ng-model="oneSupply.QtyDeviation"></div>
                <div class="col-md-12"><label class='col-sm-4'>海/河运<i></i></label>
                    <select id="select_transport" class="col-sm-8" ng-model="selected_transport" ng-options="s.id as s.transport for s in SeaOrRiver">
                    </select></div>
                <div class="col-md-12"><label class='col-sm-4'>单价<i></i></label><input class='col-sm-8 price' ng-model="oneSupply.Price"></div>

                <div class="col-md-12 input" style="position: relative;" >
                    <label class='col-sm-4'>最早起运期<i></i></label>
                    <input ng-click="showClearDateDlg(0);" class='col-sm-8 dateEdit'  id="earliestDateText" type="text" ng-model="oneSupply.LoadDateFrom">
                    <div style="position: absolute;top: 35px;right: 26px;z-index: 1000;" class="clearDateDlg earlistDateDlg" tabindex=2><!--- 防止CSS影响日期选择器 tabindex不加会导致失焦不消失--->
                        <ul><li><div id="earliestDate"></div></li></ul>
                    </div>
                </div>
                <div class="col-md-12 input" style="position: relative;" >
                    <label class='col-sm-4'>最迟起运期<i></i></label>
                    <input ng-click="showClearDateDlg(1);" class='col-sm-8 dateEdit'  id="latestDateText" type="text" ng-model="oneSupply.LoadDateTo" ng-model="startTime">
                    <div style="position: absolute;top: 35px;right: 26px;z-index: 1000;" class="clearDateDlg lastestDateDlg" tabindex=2>
                        <ul><li><div id="latestDate"></div></li></ul>
                    </div>
                </div>
                <div class="col-md-12"><label class='col-sm-4'>付款方式<i></i></label>
                    <select id="select_pay" class="col-sm-8" ng-model="selected_pay" ng-options="s.id as s.pay for s in PaymentMethod">
                    </select></div>
                <div class="col-md-12"><label class='col-sm-4'>是否代理<i></i></label>
                    <select id="select_agent" class="col-sm-8" ng-model="selected_agent" ng-options="s.id as s.agent for s in needAgent">
                    </select></div>
                <div class="col-md-12"><label class='col-sm-4'>货盘状态<i></i></label>
                    <select id="select_state" class="col-sm-8" ng-model="selected_state" ng-options="s.id as s.state for s in states">
                    </select></div>
            </form>
        </div>
        <div class="modal-footer">
            <button ng-click="save()" class="btn btn-success">保存</button>
            <button class="btn" data-dismiss="modal">关闭</button>
        </div>
    </div></div></div>

<!--搜索 start-->
<div class="modal dhide fade in" id="searchModal" style="margin-top:20px"  aria-hidden="true" data-backdrop="static">
    <div class="modal-dialog" style="width:450px;margin-top: 240px;"><div class="modal-content">
        <div class="modal-header" style="height:60px;padding-top:0px;">
            <button class="close" type="button" data-dismiss="modal" style="margin-top:5px;font-size:48px;">×</button>
            <h3>搜索船期</h3>
        </div>
        <div class="modal-body" style="height:120px;padding-left:30px;">
            <form name="content" class="contentStyle" >
                <select name="select" id="select_s1" class="col-sm-4" style="height: 33px;">
                    <option>序号</option>
                    <option>货主</option>
                    <option>货物</option>
                    <option>海/内河</option>
                    <option>吨位（吨）</option>
                    <option>出发港</option>
                    <option>到达港</option>
                    <option>最早起运时间</option>
                    <option>货盘状态</option>
                </select>
                <select name="select" id="select_s2" class="col-sm-3" style="height: 33px;">
                    <option>等于</option>
                    <option>大于</option>
                    <option>小于</option>
                </select>
                <input style="height: 33px;border: 2px solid #ccc;" class="col-sm-4 searchInput" type="text" value="">
            </form>
        </div>
        <div class="modal-footer">
            <!--<button ng-click="reset()" class="btn btn-warning pull-left">重置</button>-->
            <button ng-click="searchConfirm()" class="btn btn-success">搜索</button>
            <button class="btn" data-dismiss="modal">关闭</button>
        </div>
    </div></div></div>
<script>

    var APP = angular.module("myapp", ["ui.bootstrap","ngAnimate","utils","syzx"]);

    APP.controller("mainCtrl",function($scope,$http,$timeout,dlgPort,zhMsg)
    {
        //获取指定页
        $scope.pageChange = function()
        {	isSelectedAll = false;
            getURL("/admin/home/getSupply?page="+$scope.currentPage).then(function(res)
            {   $scope.list = res;
//                console.log(res)
                for(var i=0;i<res.length;i++)
                {
                    if(res[i].State == 0) res[i].State = "发布中";
                    if(res[i].State == 10) res[i].State = "生成订单";
                    if(res[i].State == 20) res[i].State = "失效";
                    if(res[i].SeaOrRiver == 0) res[i].SeaOrRiver = "河运";
                    if(res[i].SeaOrRiver == 1) res[i].SeaOrRiver = "海运";

                    if(res[i].PackageMethod == 1)   res[i].PackageMethod = "散装";
                    if(res[i].PackageMethod == 2)   res[i].PackageMethod = "袋装";
                    if(res[i].PackageMethod == 3)   res[i].PackageMethod = "集装箱";
                    if(res[i].PackageMethod == 4)   res[i].PackageMethod = "托板";
                    if(res[i].PackageMethod == 5)   res[i].PackageMethod = "桶装";

                    if(res[i].PaymentMethod == 10)   res[i].PaymentMethod = "卸前付清";
                    if(res[i].PaymentMethod == 20)   res[i].PaymentMethod = "卸后付清";
                    if(res[i].PaymentMethod == 30)   res[i].PaymentMethod = "月结";

                    if(res[i].NeedAgent == 0)          res[i].NeedAgent = "不需要";
                    if(res[i].NeedAgent == 1)          res[i].NeedAgent = "需要";
                }
            });
        }

        //选择全部/取消选择
        $scope.selectAll = function()
        {	isSelectedAll = !isSelectedAll;
            for(p in $scope.list)	$scope.list[p].Checked = isSelectedAll;
        }

        //检查选择，显示警告框
        function confirm(action,fn)
        {
            var ids = [];

            for(var i=0;i<$scope.list.length;i++)	if($scope.list[i].Checked == 1)	ids.push($scope.list[i].ID);// += $scope.list[i].ID + ",";

            if(ids.length == 0)
            {	msgBox("请先选择。");
                return;
            }

            msgBox(action + "指定的记录，是否确定？",MSG_INFORMATION,MSG_CONFIRM).then(function(){fn(ids);});
        }

        //点击“删除”按钮
        $scope.del = function()
        {
            confirm("删除",function(value)
            {
                getURL("/admin/Supply/del/?ID=" + value.join(",")).then(function() { $scope.pageChange(); });
            });
        }

        //刷新
        $scope.refresh = function()
        {
            window.location.reload();
        };
        //点击行打开对话框
        var clearPortName = "";
        $scope.show = function(id,$event)
        {
            //判断是否为火狐浏览器
            if (navigator.userAgent.indexOf("Firefox") >= 0)
            {   if($event.target.tagName == "INPUT") return;
            }
            else
            {   if(event.srcElement.tagName == "INPUT")	return;
            }

            getURL("/admin/home/GetoneSupply/?ID=" + id).then(function(data)
            {	$scope.oneSupply = data;
                //包装形式
                $scope.packageMethod = [{pack:"散装",id:"1"},{pack:"袋装",id:"2"},{pack:"集装箱",id:"3"},{pack:"托板",id:"4"},{pack:"桶装",id:"5"}];
                $scope.selected_pack = data.PackageMethod;  //id的值，区分类型

                $scope.SeaOrRiver = [{transport:"河运",id:"0"},{transport:"海运",id:"1"}];
                $scope.selected_transport = data.SeaOrRiver;
                //付款方式
                $scope.PaymentMethod = [{pay:"卸前付清",id:"10"},{pay:"卸后付清",id:"20"},{pay:"月结",id:"3"}];
                $scope.selected_pay = data.PaymentMethod;
                //是否需要代理
                $scope.needAgent = [{agent:"不需要",id:"0"},{agent:"需要",id:"1"}];
                $scope.selected_agent = data.NeedAgent;
                //状态选择select
                $scope.states = [{state:'发布中',id:"0"},{state:'生成订单',id:"10"},{state:'失效',id:"20"}];
                $scope.selected_state = data.State;

                if(data.State == 0) data.State = "发布中";
                if(data.State == 10) data.State = "生成订单";
                if(data.State == 20) data.State = "失效";
                if(data.SeaOrRiver == 0) data.SeaOrRiver = "河运";
                if(data.SeaOrRiver == 1) data.SeaOrRiver = "海运";

                $("#myModal").modal("show");
            });
        };

        //点击“新增”按钮
        $scope.add = function()
        {
            $scope.packageMethod = [{pack:"散装",id:"1"},{pack:"袋装",id:"2"},{pack:"集装箱",id:"3"},{pack:"托板",id:"4"},{pack:"桶装",id:"5"}];
            $scope.selected_pack = -1;  //id的值，区分类型
            $scope.SeaOrRiver = [{transport:"河运",id:"0"},{transport:"海运",id:"1"}];
            $scope.selected_transport = -1;
            //付款方式
            $scope.PaymentMethod = [{pay:"卸前付清",id:"10"},{pay:"卸后付清",id:"20"},{pay:"月结",id:"3"}];
            $scope.selected_pay = -1;
            //是否需要代理
            $scope.needAgent = [{agent:"不需要",id:"0"},{agent:"需要",id:"1"}];
            $scope.selected_agent = -1;
            //状态选择select
            $scope.states = [{state:'发布中',id:"0"},{state:'生成订单',id:"10"},{state:'失效',id:"20"}];
            $scope.selected_state = -1;

            $scope.oneSupply = {ID:0,UserName:"",State:""};
            $("#myModal").modal("show");
        };

        //=========================货主用户名或手机号搜索 开始
        $scope.showUserDlg = function(input)
        {
            var type = $(".selectType").val();
            getURL("/admin/supply/getUserInfo?INPUT="+input).then(function(data)
            {
                $scope.inputSelect = data;
                if(input != "") $("#inputSelectList").slideDown();
                else     $("#inputSelectList").slideUp();
            });

            $scope.inputSelectList = function(index)
            {   var text = $("#inputSelectList li").eq(index).html();
                $("#userSelectText").val(text);
            }
            //判断是否失去焦点
            $(".modal").click(function()
            {   if( $("#userSelectText").is(":focus") !=true )  $("#inputSelectList").slideUp();
            });
        }

        //点击“保存”按钮
        $scope.save = function()
        {
            //先判断货主名是否存在
            var type = $(".selectType").val();
            var user = $("#userSelectText").val();
            getURL("/admin/supply/userExist/?USER="+user+"&TYPE="+type ).then(function(data)
            {
                //货主名存在，保存数据
                var package = $("#select_pack").find("option:selected").text();
                var transport = $("#select_transport").find("option:selected").text();
                var pay = $("#select_pay").find("option:selected").text();
                var agent = $("#select_agent").find("option:selected").text();
                var state = $("#select_state").find("option:selected").text();
                var seaOrRiver = $(".seaOrRiver").val();
                var seaOrRiver = $(".seaOrRiver").val();

                if(package == "散装")   package = "1";
                if(package == "袋装")   package = "2";
                if(package == "集装箱")   package = "3";
                if(package == "托板")   package = "4";
                if(package == "桶装")   package = "5";

                if(transport == "河运")     transport = "0";
                if(transport == "海运")     transport = "1";

                if(pay == "卸前付清")   pay = "10";
                if(pay == "卸后付清")   pay = "20";
                if(pay == "月结")       pay = "30";

                if(agent == "需要")     agent = "1";
                if(agent == "不需要")   agent = "0";

                if(state == "发布中") state = "0";
                if(state == "生成订单") state = "10";
                if(state == "失效") state = "20";

                var data = {
                    ID 			        : $scope.oneSupply.ID,
                    UserName               :$(".userName").val(),
                    GoodsName          :  $(".goodsName").val(),
                    PackageMethod      : package,
                    Qty                 :$(".qty").val(),
                    QtyDeviation        :$(".qtyDeviation").val(),
                    SeaOrRiver          :transport,
                    Price               :$(".price").val(),
                    FromPortName        :$("#fromPort").val(),
                    ToPortName          :$("#toPort").val(),
                    LoadDateFrom        :$("#earliestDateText").val(),
                    LoadDateTo          :$("#latestDateText").val(),
                    PaymentMethod       :pay,
                    NeedAgent            :agent,
                    State                 :state
                };
                console.log($(".userName").val());
                return;
                postURL("/admin/Supply/saves",data).then(function(data)
                        {
                            $scope.pageChange();
                            $("#myModal").modal("hide");
                            msgBox("保存完成。")
                        },
                        function(err) { zhMsg.alert(err.message?err.message:err); });
            },function(err){ zhMsg.alert("货主请选择用户名或手机号！");exist = true;});
        };

        //=====================分页 开始 ==================
        var isSelectedAll = false;
        $scope.currentPage = 1;		//初始当前页
        $scope.SupplyNumber = 0;
        //总记录数
        getURL("/admin/Supply/getCount").then(function(data) { $scope.SupplyNumber = data;console.log(data)});
        //获取数据
        $scope.pageChange();
        //======================分页 结束==========================

        //======================= 空船时间选择对话框相关函数 --- 开始 ========================
        $(".clearDateDlg").hide("fast");
        var clearDateTimer = null;
        $scope.showClearDateDlg = function(num)
        {
            if(num == 0)
            {
                $_dateDlg = $(".earlistDateDlg");
                $_dateText = $("#earliestDateText")
                $_date = $("#earliestDate");
            }
            if(num == 1)
            {
                $_dateDlg = $(".lastestDateDlg");
                $_dateText = $("#latestDateText")
                $_date = $("#latestDate");
            }

            $_date.datepicker({"onSelect":function(value,obj)
            {	$(this).fadeIn();
                $scope.search.ClearDateFrom = value;
                $_dateDlg.hide("fast");
                $_dateText.val($scope.search.ClearDateFrom);
            }});

            $_dateDlg.show("fast").focus().unbind().bind("blur",function()
            {	//失去焦点，如果是点击日期，不关闭选择框，否则就关闭它
                var oldFrom = $scope.search.ClearDateFrom;

                $timeout.cancel(clearDateTimer);
                clearDateTimer = $timeout(function()
                {	//是点击了datapicker
                    if(oldFrom != $scope.search.ClearDateFrom)
                        $_dateDlg.focus();
                    else
                        $_dateDlg.hide("fast");
                },200);
            });

            $_date.datepicker("setDate",$scope.search.ClearDateFrom);
        }

        //======================= 空船时间选择对话框相关函数 --- 结束 ========================

        //======================= 出发/到达 港口选择对话框 开始
        $scope.showPortDlg = function(port)
        {
            if(port == 0)
            {var $portBox = $("#fromPortBox");
                var $port = $("#fromPort");
            }
            else
            {var $portBox = $("#toPortBox");
                var $port = $("#toPort");
            }
            dlgPort.show($portBox).then(function(data)
            {
                //$scope.search.Clear
                $scope.search.ClearPortID = data.portID;

                if(data.provinceName == data.regionName)
                    $port.val(data.provinceName + " " + data.portName);
                else
                    $port.val(data.provinceName + " " + data.regionName + " " + data.portName);
            });
        }
        //======================= 出发/到达 港口选择对话框 结束

        //=============键盘事件函数 开始========
        $(function () {
            var input = $("#userSelectText");
            var suggestions = $("#inputSelectList");
            var currentindex = -1;
            input.keyup(function (evn) {
                var keyBol = $("#inputSelectList").css("display") == "block";
                if (evn.keyCode == 38 && keyBol) {
                    movethis(true);
                }
                else if (evn.keyCode == 40 && keyBol) {
                    movethis(false);
                }
                else if (evn.keyCode == 13) {
                    $("#inputSelectList").slideUp();
                }
            });
            var movethis = function(up)
            {
                var list = $("#inputSelectList li")
                var size = list.length;
                var textvalue = $(list[currentindex+1]).text();

                currentindex = currentindex + (up ? -1 : 1);
                if(currentindex >= size) currentindex = 0;
                if(currentindex < 0 ) currentindex = size;

                list.removeClass("inputLiAct");
                $(list[currentindex]).addClass("inputLiAct");
                $("#userSelectText").val(textvalue);
            }

            //鼠标滑过事件
            $("#inputSelectList").mouseover(function () {
                $("#inputSelectList li").mouseover(function () {
                    $(this).addClass("inputLiAct");
                    currentindex = $("#inputSelectList li").index(this);
                }).mouseout(function () {
                    $(this).removeClass("inputLiAct");
                });
            });

        })
        //=============键盘事件函数 结束========
    });
</script>

</body>
</html>

